        // è‡ªåŠ¨å®ŒæˆåŠŸèƒ½
        const universityInput = document.getElementById('university');
        const autocompleteList = document.getElementById('autocompleteList');
        
        universityInput.addEventListener('input', function() {
            const query = this.value.toLowerCase().trim();
            if (query.length < 1) {
                autocompleteList.style.display = 'none';
                return;
            }
            
            const matches = Object.keys(exemptionData).filter(school => 
                school.toLowerCase().includes(query) || 
                exemptionData[school].english.toLowerCase().includes(query)
            );
            
            if (matches.length > 0) {
                autocompleteList.innerHTML = matches.slice(0, 8).map(school => 
                    `<div class="autocomplete-item" onclick="selectUniversity('${school}')">${school} (${exemptionData[school].english})</div>`
                ).join('');
                autocompleteList.style.display = 'block';
            } else {
                autocompleteList.style.display = 'none';
            }
        });

        function selectUniversity(school) {
            universityInput.value = school;
            autocompleteList.style.display = 'none';
        }

        // ç‚¹å‡»å…¶ä»–åœ°æ–¹éšè—è‡ªåŠ¨å®Œæˆåˆ—è¡¨
        document.addEventListener('click', function(e) {
            if (!e.target.closest('.autocomplete-container')) {
                autocompleteList.style.display = 'none';
            }
        });

        // æŸ¥è¯¢å‡½æ•°
        function doQuery() {
            const university = document.getElementById('university').value.trim();
            const program = document.getElementById('program').value.trim();
            const degree = document.getElementById('degree').value;
            const startYear = parseInt(document.getElementById('startYear').value);
            const graduateYear = parseInt(document.getElementById('graduateYear').value);

            if (!university || !program || !degree || !startYear || !graduateYear) {
                alert('è¯·å¡«å†™å®Œæ•´ä¿¡æ¯');
                return;
            }

            if (graduateYear <= startYear) {
                alert('æ¯•ä¸šå¹´ä»½å¿…é¡»æ™šäºå…¥å­¦å¹´ä»½');
                return;
            }

            document.getElementById('loading').style.display = 'block';
            document.getElementById('resultSection').style.display = 'none';

            setTimeout(() => {
                const result = checkQualification(university, program, degree, startYear, graduateYear);
                showResult(result);
            }, 1200);
        }

        // éªŒè¯å‡½æ•°
        function checkQualification(university, program, degree, startYear, graduateYear) {
            const schoolData = exemptionData[university];
            if (!schoolData) {
                return {
                    status: 'not-found',
                    title: 'âŒ å­¦æ ¡ä¸åœ¨å…è®¤è¯æ¸…å•ä¸­',
                    content: `å¾ˆæŠ±æ­‰ï¼Œ"${university}" ä¸åœ¨æ–°è¥¿å…°ç§»æ°‘å±€å…NZQAè®¤è¯æ¸…å•ä¸­ã€‚<br><br>å½“å‰æ•°æ®åº“åŒ…å«118æ‰€ä¸­å›½å¤§å­¦ï¼Œå¦‚æœæ‚¨çš„å­¦æ ¡ä¸åœ¨åˆ—è¡¨ä¸­ï¼Œè¯´æ˜è¯¥æ ¡ä¸åœ¨INZå®˜æ–¹å…è®¤è¯æ¸…å•å†…ï¼Œéœ€è¦é€šè¿‡NZQAè¿›è¡Œå­¦å†è®¤è¯ã€‚`
                };
            }

            const qualification = schoolData.qualifications[degree];
            if (!qualification) {
                return {
                    status: 'not-found', 
                    title: 'âŒ è¯¥å­¦ä½ä¸åœ¨å…è®¤è¯èŒƒå›´',
                    content: `"${university}" çš„è¯¥å­¦ä½ç±»å‹ä¸åœ¨å…è®¤è¯æ¸…å•ä¸­ã€‚`
                };
            }

            const studyYears = graduateYear - startYear;
            const issues = [];

            // å­¦åˆ¶æ£€æŸ¥
            if (studyYears < qualification.minYears) {
                issues.push(`å­¦åˆ¶å¹´é™ä¸è¶³ï¼šè¦æ±‚${qualification.minYears}å¹´ï¼Œå®é™…${studyYears}å¹´`);
            }

            // æ—¶é—´æ£€æŸ¥ - 2004å¹´å¼€å§‹ç”Ÿæ•ˆ
            if (startYear < qualification.validFrom) {
                issues.push(`å…¥å­¦æ—¶é—´è¿‡æ—©ï¼šå…è®¤è¯ä»${qualification.validFrom}å¹´å¼€å§‹ç”Ÿæ•ˆ`);
            }

            // æ£€æŸ¥æ’é™¤çš„ä¸“ä¸šç±»å‹
            const excludedPrograms = {
                bachelor: ["æˆäººé«˜ç­‰æ•™è‚²", "é«˜ç­‰æ•™è‚²è‡ªå­¦è€ƒè¯•", "ç¬¬äºŒå­¦å£«å­¦ä½", "ç½‘ç»œæ•™è‚²", "å‡½æˆ"],
                master: ["MBA", "Executive MBA", "å·¥å•†ç®¡ç†ç¡•å£«", "æœ¬ç¡•è¿è¯»"],
                phd: ["å·¥å•†ç®¡ç†åšå£«", "ç¡•åšè¿è¯»"]
            };

            const excluded = excludedPrograms[degree] || [];
            for (const ex of excluded) {
                if (program.includes(ex)) {
                    issues.push(`ä¸“ä¸šç±»å‹ä¸ç¬¦ï¼š${ex}ä¸åœ¨å…è®¤è¯èŒƒå›´`);
                }
            }

            if (issues.length > 0) {
                return {
                    status: 'not-eligible',
                    title: 'âŒ ä¸ç¬¦åˆå…è®¤è¯æ¡ä»¶',
                    content: `å­˜åœ¨ä»¥ä¸‹é—®é¢˜ï¼š<br><br>${issues.map(i => `â€¢ ${i}`).join('<br>')}<br><br>å»ºè®®é€šè¿‡NZQAè¿›è¡Œæ­£å¼å­¦å†è®¤è¯ã€‚`
                };
            }

            const degreeNames = {
                bachelor: 'å­¦å£«å­¦ä½',
                master: 'ç¡•å£«å­¦ä½', 
                phd: 'åšå£«å­¦ä½'
            };

            return {
                status: 'eligible',
                title: 'âœ… ç¬¦åˆå…è®¤è¯æ¡ä»¶',
                content: `æ­å–œï¼æ‚¨çš„å­¦å†ç¬¦åˆæ–°è¥¿å…°ç§»æ°‘å±€å…NZQAè®¤è¯æ¡ä»¶ã€‚<br><br><strong>è¯¦ç»†ä¿¡æ¯ï¼š</strong><br>â€¢ å­¦æ ¡ï¼š${schoolData.english}<br>â€¢ å­¦ä½ï¼š${degreeNames[degree]}<br>â€¢ NZQFç­‰çº§ï¼šLevel ${qualification.nzqf}<br>â€¢ SMCæŠ€èƒ½ç§»æ°‘ç§¯åˆ†ï¼š${qualification.points}åˆ†<br>â€¢ å­¦åˆ¶ï¼š${studyYears}å¹´<br><br><strong>é‡è¦æé†’ï¼š</strong><br>â€¢ å¿…é¡»æä¾›å­¦ä½è¯ä¹¦å’Œæ¯•ä¸šè¯ä¹¦åŸä»¶<br>â€¢ éœ€è¦å®˜æ–¹è‹±æ–‡ç¿»è¯‘ä»¶<br>â€¢ å­¦å£«å­¦ä½éœ€æ ‡æ³¨"æ™®é€šé«˜ç­‰æ•™è‚²"<br>â€¢ ç ”ç©¶ç”Ÿå­¦ä½éœ€æ ‡æ³¨"æ¯•ä¸šè®ºæ–‡ç­”è¾©é€šè¿‡"`
            };
        }

        // æ˜¾ç¤ºç»“æœ
        function showResult(result) {
            document.getElementById('loading').style.display = 'none';
            
            const section = document.getElementById('resultSection');
            const title = document.getElementById('resultTitle');
            const content = document.getElementById('resultContent');
            
            section.className = 'result-section';
            
            if (result.status === 'eligible') {
                section.classList.add('result-success');
            } else if (result.status === 'conditional') {
                section.classList.add('result-warning');  
            } else {
                section.classList.add('result-error');
            }
            
            title.innerHTML = result.title;
            content.innerHTML = result.content;
            section.style.display = 'block';
            
            section.scrollIntoView({ behavior: 'smooth' });
        }
    </script>
</body>
</html>
